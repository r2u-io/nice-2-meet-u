{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2019"
    ],
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./src",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "noImplicitAny": true
  }
}

